package application;

public class Program {

	public static void main(String[] args) {

		int n = 71;

		int inicio = 0;
		int fim = 0;
		int intervalo = 2;

		for (int i = 0; i < 71; i += intervalo) {
			inicio = i - 1;
			fim += intervalo;
			System.out.printf("%d, %d\n", inicio, fim);
		}

	}

}
